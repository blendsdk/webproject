{
  "name": "codegen",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "lint": "tslint -p ./tsconfig.json --fix",
    "mktypes": "node ./dist/bin/mkdb.js --gen-types",
    "mkdb": "node ./dist/bin/mkdb.js --gen-database",
    "build": "yarn build:src && yarn mktypes",
