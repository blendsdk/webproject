    "watch": "rm -fR ./dist && tsc -w ",
    "test": "jest --detectOpenHandles --passWithNoTests"
  },
  "dependencies": {
    "@blendsdk/express": "^1.1.7",
    "@blendsdk/codekit": "^1.1.4",
    "@blendsdk/schemakit": "^1.1.4",
    "@blendsdk/sqlkit": "^1.1.4",
    "@blendsdk/stdlib": "^1.1.4",
    "chalk": "^2.4.2"
  }
}